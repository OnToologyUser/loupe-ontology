                    rdfs:domain core:VocabularyPartition ;
                    rdfs:range rdfs:Resource ;
                    rdfs:label "aboutNamespace"@en .


###  http://ont-loupe.linkeddata.es/def/core#aboutProperty
core:aboutProperty rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( core:ClassPropertyPartition
                                               core:PropertyPartition
                                             )
                               ] ;
                   rdfs:range rdf:Property ;
                   rdfs:label "aboutProperty"@en .


